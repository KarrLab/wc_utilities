alias ls="ls -G"
alias h=history
alias m=more
alias t='tail -3'
alias g=grep
alias j=jobs
alias f=find
alias p=pushd
alias he='head -3'
alias d='dirs -v'
export LS_COLORS='di=0;35:'
export LS_OPTIONS='--color=auto'
alias ls='ls $LS_OPTIONS'
alias ll="ls -lt"

export PATH=$PATH:.

PS1="\[\033[1;34m\][\# \u@\h:\w]$\[\033[0m\] "



